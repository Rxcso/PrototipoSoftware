@using WebApplication4.Models
@{
    Layout = null;
    int itemEliminar = int.Parse(Request["itemEliminar"]);
    List<PaqueteEntradas> carrito = (List<PaqueteEntradas>)Session["CarritoVendedor"];
    carrito.RemoveAt(itemEliminar);
    Session["CarritoVendedor"] = carrito;
    Json.Write("Item Eliminado", Response.Output);
}
