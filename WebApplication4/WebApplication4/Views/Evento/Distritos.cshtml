@{
    Layout = null;
    var db = new WebApplication4.Models.inf245netsoft();
    int depId = Request.QueryString[0].AsInt();
    var data = from obj in db.Region
               where obj.idRegPadre == depId
               select new { IdRegion = obj.idRegion, Nombre = obj.nombre };
    Json.Write(data, Response.Output);
}